<h1>New User</h1>

<%= form_for :user do |f| %>
  <p>

  </p>

  <table border="1">
    <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>E-Mail</th>
    <th>Mobile Phone</th>
    <th>Notes</th>      
    </tr>
  <tr>
    <td><%= f.text_field :firstname %></td>
    <td><%= f.text_field :lastname %></td>
    <td><%= f.text_field :mobilephone %></td>
    <td><%= f.text_field :email %></td>
    <td><%= f.text_field :notes %></td>
  </tr>
  </table>

    <%= f.submit %>

<% end %>